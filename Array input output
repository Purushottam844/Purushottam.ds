#include <stdio.h>

int main() {
    int menu[16];
    int i;

    // Input 16 integers into the array
    for (i = 0; i < 16; i++) {
        printf("Input array value %d: ", i + 1);
        scanf("%d", &menu[i]);
    }

    // Output the array
    printf("The array values are: ");
    for (i = 0; i < 16; i++) {
        printf("%d ", menu[i]);
    }
    printf("\n");

    return 0;
}
